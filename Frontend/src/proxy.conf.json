{
  "/api/login": {
    "target": "http://localhost:8080",
    "secure": false,
    "pathRewrite": {
      "^/api/login": "/oauth2/authorization/google"
    }
  },
  "/api": {
    "target": "http://localhost:8080",
    "secure": false
  },
  "/o": {
    "target": "https://accounts.google.com",
    "secure": true,
    "changeOrigin": true
  }
}
